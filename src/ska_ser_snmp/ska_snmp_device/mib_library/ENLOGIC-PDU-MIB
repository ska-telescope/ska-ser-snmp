PduExternalSensorConfigEntry ::= SEQUENCE {
	pduExternalAssetULevel
		DisplayString,
	pduExternalAssetID
		DisplayString,
	pduExternalAssetBrand
		DisplayString,
	pduExternalAssetColorStandby
		Unsigned32,
    pduExternalAssetColorConnected
		Unsigned32,
	pduExternalMasterOrientation
		Unsigned32,           
	}

pduExternalAssetULevel  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  	read-only
	STATUS 	current
	DESCRIPTION 
		"U-level position of Asset Module along with respective TagID"
	::= { pduExternalAssetManagerEntry 1 }

pduExternalAssetID  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  	read-write
	STATUS 	current
	DESCRIPTION 
		"Asset ID (20 bytes alphanumeric ,No Special Char).Enter HEX and Read HEX values"
	::= { pduExternalAssetManagerEntry 2 }
pduExternalAssetBrand  OBJECT-TYPE
	SYNTAX 	DisplayString
	MAX-ACCESS  	read-write
	STATUS 	current
	DESCRIPTION 
		"Asset Brand(20 bytes alphanumeric ,No Special Char).Enter HEX and Read HEX values"
	::= { pduExternalAssetManagerEntry 3 }
pduExternalAssetColorStandby  OBJECT-TYPE
	SYNTAX 	Unsigned32
	MAX-ACCESS  	read-write
	STATUS 	current
	DESCRIPTION 
		"U-Level Light Color
		Not Bright 0, Red 1
		Purple 2, Yellow 3
		Green 4, Cyan 5
		Blue 6, White 7
		Red Flash 8, Purple Flash 9
		Yellow Flash 10, Green Flash 11
		Cyan Flash 12, Blue Flash 13
		White Flash 14	"
		
	::= { pduExternalAssetManagerEntry 4 }
pduExternalAssetColorConnected  OBJECT-TYPE
	SYNTAX 	Unsigned32
	MAX-ACCESS  	read-write
	STATUS 	current
	DESCRIPTION 
		"U-Level Light Color
		Not Bright 0, Red 1
		Purple 2, Yellow 3
		Green 4, Cyan 5
		Blue 6, White 7
		Red Flash 8, Purple Flash 9
		Yellow Flash 10, Green Flash 11
		Cyan Flash 12, Blue Flash 13
		White Flash 14	"
		
	::= { pduExternalAssetManagerEntry 5 }
pduExternalMasterOrientation  OBJECT-TYPE
	SYNTAX 	Unsigned32
	MAX-ACCESS  	read-write
	STATUS 	current
	DESCRIPTION 
		"Master Orientation ( 0 - bottom to top)(1- top to bottom) "
	::= { pduExternalAssetManagerEntry 6 }

--  Smart Cabinet

