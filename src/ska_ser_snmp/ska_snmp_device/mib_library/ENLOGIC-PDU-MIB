	SYNTAX 	Integer32 (0..255)
	MAX-ACCESS  	read-only
	STATUS 	current
	DESCRIPTION 
		"CircuitBreaker CurrentRating in hundreths of ampere"
	::= { pduCircuitBreakerPropertiesEntry 4 }

